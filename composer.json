{
    "name": "neliserp/inventory",
    "description": "Inventory package.",
    "license": "MIT",
    "require": {
        "illuminate/support": "^7.4",
        "neliserp/core": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Neliserp\\Inventory\\": "src/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Neliserp\\Inventory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neliserp\\Inventory\\InventoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        }
    ]
}
